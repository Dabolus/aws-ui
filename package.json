{
  "name": "localui",
  "description": "A simple UI to interact with real or emulated AWS services (localstack, minio, etc.)",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": "Giorgio Garasto <giorgio@garasto.me>",
  "repository": "https://github.com/Dabolus/localui.git",
  "homepage": "https://github.com/Dabolus/localui",
  "funding": "https://github.com/sponsors/Dabolus",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently 'npm:dev:*'",
    "dev:remix": "LOCALUI_SERVICES_ENDPOINT=http://localhost:4566 LOCALUI_ACCESS_KEY_ID=accesskeyid LOCALUI_SECRET_ACCESS_KEY=secretaccesskey remix dev --manual",
    "dev:localstack": "docker compose up --force-recreate --build --renew-anon-volumes --remove-orphans",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "upgrade-interactive": "ncu --interactive"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.485.0",
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/client-sqs": "^3.485.0",
    "@aws-sdk/lib-dynamodb": "^3.485.0",
    "@aws-sdk/lib-storage": "^3.485.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@mui/x-data-grid": "^6.18.7",
    "@mui/x-tree-view": "^6.17.0",
    "@remix-run/css-bundle": "^2.4.1",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@remix-run/serve": "^2.4.1",
    "@remix-run/server-runtime": "^2.4.1",
    "csv-parse": "^5.5.3",
    "docx-preview": "^0.3.0",
    "fuse.js": "^7.0.0",
    "material-ui-popup-state": "^5.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone-esm": "^15.0.1",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.4.1",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-syntax-highlighter": "^15",
    "concurrently": "^8.2.2",
    "npm-check-updates": "^16.14.12",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
