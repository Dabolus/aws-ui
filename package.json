{
  "name": "aws-ui",
  "description": "A simple UI to interact with real or emulated AWS services (localstack, minio, etc.)",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": "Giorgio Garasto <giorgio@garasto.me>",
  "repository": "https://github.com/Dabolus/aws-ui.git",
  "homepage": "https://github.com/Dabolus/aws-ui",
  "funding": "https://github.com/sponsors/Dabolus",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently 'npm:dev:*'",
    "dev:remix": "AWS_UI_SERVICES_ENDPOINT=http://localhost:4566 AWS_UI_ACCESS_KEY_ID=accesskeyid AWS_UI_SECRET_ACCESS_KEY=secretaccesskey remix dev --manual",
    "dev:localstack": "docker compose up --force-recreate --build --renew-anon-volumes --remove-orphans",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.441.0",
    "@aws-sdk/client-s3": "^3.440.0",
    "@aws-sdk/lib-dynamodb": "^3.441.0",
    "@aws-sdk/lib-storage": "^3.441.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.16",
    "@mui/x-data-grid": "^6.17.0",
    "@mui/x-tree-view": "^6.17.0",
    "@remix-run/css-bundle": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@remix-run/serve": "^2.2.0",
    "@remix-run/server-runtime": "^2.2.0",
    "csv-parse": "^5.5.2",
    "docx-preview": "^0.1.20",
    "fuse.js": "^7.0.0",
    "material-ui-popup-state": "^5.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone-esm": "^15.0.1",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.2.0",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-syntax-highlighter": "^15",
    "concurrently": "^8.2.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
